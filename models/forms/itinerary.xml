<?xml version="1.0" encoding="utf-8"?><form>		<fieldset name="myfields"> 		<field name="clef" type="hidden" label="JGRID_HEADING_ID" />		<field name="clef_planificateur" type="hidden" label="JGRID_HEADING_ID"/>		<field name="latitude" type="hidden" label="COM_ITINERARY_FIELD_POSITION_LABEL" description="COM_ITINERARY_FIELD_DEPART_LATITUDE_DESC" size="40" required="false" readonly="false" validate="number" min="-90" max="90"/>		<field name="longitude" type="hidden" label="COM_ITINERARY_FIELD_LONGITUDE_LABEL" description="COM_ITINERARY_FIELD_DEPART_LONGITUDE_DESC" size="40" required="false" readonly="false" validate="number" min="-180" max="180"/>		<field name="date_creation" type="hidden" label="COM_ITINERARY_FIELD_DATE_CREATION_LABEL" description="COM_ITINERARY_FIELD_DATE_CREATION_DESC" class="form-control" readonly="true"  disabled="true"/>		<field name="date_modification" type="hidden" label="COM_ITINERARY_FIELD_DATE_MODIFICATION_LABEL" description="COM_ITINERARY_FIELD_DATE_MODIFICATION_DESC" class="form-control" readonly="true"  disabled="true"/>				<field	name="titre" type="text" label="JGLOBAL_TITLE" description="COM_ITINERARY_FIELD_PARCOURS_TITLE_DESC" maxlength="100" size="40" required="true"/>		<field	name="description" type="textarea" label="JGLOBAL_DESCRIPTION" description="COM_ITINERARY_FIELD_PARCOURS_DESCRIPTION_DESC" maxlength="4000" required="true"/>				<field	name="theme" type="list" label="COM_ITINERARY_FIELD_THEME_LABEL" description="COM_ITINERARY_FIELD_THEME_DESC" required="true" validate="options">			<option>COM_ITINERARY_FIELD_SELECT_THEME</option>                        <option value="1">COM_ITINERARY_THEME_HISTORY</option>			<option value="2">COM_ITINERARY_THEME_PARK</option>			<option value="3">COM_ITINERARY_THEME_URBAN</option>			<option value="4">COM_ITINERARY_THEME_ARCHITECTURE</option>			<option value="5">COM_ITINERARY_THEME_LEISURE</option>                        <option value="6">COM_ITINERARY_THEME_STREET_ART</option>                        <option value="7">COM_ITINERARY_THEME_OTHERS</option>		</field>				<field	name="langue" type="contentlanguage" label="JFIELD_LANGUAGE_LABEL" description="COM_ITINERARY_FIELD_PARCOURS_LANGUAGE_DESC" class="" required="true">			<option>JOPTION_SELECT_LANGUAGE</option>		</field>		<field	name="duree" type="list" label="COM_ITINERARY_FIELD_DUREE_LABEL" description="COM_ITINERARY_FIELD_DUREE_DESC" required="true" validate="options">			<option>COM_ITINERARY_FIELD_SELECT_VALUE</option>			<option value="0">COM_ITINERARY_FIELD_DUREE_VALUE_FREE</option>			<option value="30">30min.</option>			<option value="60">1h</option>			<option value="90">1h30</option>			<option value="120">2h</option>			<option value="150">2h30</option>			<option value="180">3h</option>			<option value="210">3h30</option>			<option value="240">4h</option>		</field>						<field	name="payant" 				type="radio" 				label="COM_ITINERARY_FIELD_ACCESS_CATEGORY_LABEL" 				description="COM_ITINERARY_FIELD_ACCESS_CATEGORY_DESC"				default="0" 				required="true" 				validate="boolean"				class="btn-group" >			<option value="0">COM_ITINERARY_FIELD_FREE_LABEL</option>			<option value="1">COM_ITINERARY_FIELD_CHARGE_LABEL</option>		</field>				<field	name="tarif" type="text" showon="payant:1"	label="COM_ITINERARY_FIELD_TARIF_LABEL" description="COM_ITINERARY_FIELD_TARIF_DESC" required="false" validate="number" min="0" max="500"/>				<field	name="type_parcours" 				type="radio" 				label="COM_ITINERARY_FIELD_PARCOURS_TYPE_LABEL" 				description="COM_ITINERARY_FIELD_PARCOURS_TYPE_DESC"				default="0" 				required="true" 				validate="boolean"				class="btn-group">			<option value="0">COM_ITINERARY_FIELD_PROMENADE_TYPE_LABEL</option>			<option value="1">COM_ITINERARY_FIELD_CIRCUIT_TYPE_LABEL</option>		</field>		<field	name="activation_planificateur" 				type="radio" 				label="COM_ITINERARY_FIELD_PARCOURS_ACTIVATED_LABEL" 				description="COM_ITINERARY_FIELD_PARCOURS_ACTIVATED_DESC"				default="0" 				required="true" 				validate="boolean"				class="btn-group-yesno">			<option value="0">JNO</option>			<option value="1">JYES</option>		</field>				<field	name="telechargeable" 				type="radio" 				label="COM_ITINERARY_FIELD_PARCOURS_DOWNLOADABLE_LABEL" 				description="COM_ITINERARY_FIELD_PARCOURS_DOWNLOADABLE_DESC"				default="0" 				required="true" 				validate="boolean"				class="btn-group-yesno">			<option value="0">JNO</option>			<option value="1">JYES</option>		</field>				<field name="illustrations" type="ItinerarySelectImage" accept="image/*" label="COM_ITINERARY_FIELD_MESSAGE_ILLUSTRATIONS_LABEL" description="COM_ITINERARY_FIELD_MESSAGE_ILLUSTRATIONS_DESC"  size="40" />		<field name="MAX_FILE_SIZE" type="hidden" value="610000" />	</fieldset></form>